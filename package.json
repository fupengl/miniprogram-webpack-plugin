{
  "name": "miniprogram-webpack-plugin",
  "version": "0.1.0",
  "description": "微信小程序 Webpack 插件",
  "scripts": {
    "start": "jest test --watch",
    "test": "jest test",
    "test:webpack": "cd test && webpack --mode=development --progress",
    "test:webpack:ts": "cd test && TEST_EXT=ts webpack --progress",
    "lint": "eslint --ext .js src/ --fix",
    "prebuild": "rimraf ./lib && mkdirp ./lib",
    "build:watch": "babel src -o lib/index.js -w",
    "build": "npx babel src --out-dir lib",
    "preversion": "npm test && npm build",
    "prepublish": "npm run build"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "*.md"
  ],
  "keywords": [
    "miniprogram",
    "weapp",
    "webpack",
    "webpack-plugin",
    "miniprogram-webpack",
    "wxapp-webpack"
  ],
  "author": "fupengl",
  "repository": "fupengl/miniprogram-webpack-plugin",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/plugin-transform-destructuring": "^7.1.3",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "rimraf": "^2.6.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "fs-extra": "^7.0.1",
    "globby": "^8.0.1",
    "webpack-sources": "^1.3.0"
  }
}
